{
  "extends": "expo/tsconfig.base",

  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@env": [
        "@types/env.d.ts"
      ],
        "@/*": [
          "src/*"
        ],
    },
    "typeRoots": [
      "./node_modules/@types",
      "./@types"
    ],
    "strict": true
  },
  "include": ["src/**/*", "@types/**/*"],
}
